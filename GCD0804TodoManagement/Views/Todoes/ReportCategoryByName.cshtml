@model IEnumerable<GCD0804TodoManagement.ViewModels.CategoryQuantityByName>
@{
	ViewBag.Title = "ReportCategoryByName";
}

<h2>ReportCategoryByName</h2>

<table class="table">
	<thead class="thead-light">
		<tr>
			<th scope="col">Category Name</th>
			<th scope="col">Category Quantity</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<th scope="row">@item.CategoryName</th>
				<td>@item.CategoryQuantity</td>

			</tr>
		}
	</tbody>
</table>

